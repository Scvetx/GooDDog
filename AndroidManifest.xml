<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ss.scvex.test21_testapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="ss.scvex.test21_testapp.MainTest21Activity"
            android:label="@string/app_name" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name="Training" 
            android:screenOrientation="portrait" 
            android:parentActivityName="ss.scvex.test21_testapp.MainTest21Activity">
        </activity>
        
        <activity 
            android:name="Accessories" 
            android:screenOrientation="portrait"
     		android:parentActivityName="ss.scvex.test21_testapp.MainTest21Activity">
        </activity>
        
        <activity 
            android:screenOrientation="portrait" 
            android:name="TrainingDescription"
            android:parentActivityName="Training">
        </activity>
        <activity 
            android:name="WebAccessories"
            android:parentActivityName="Accessories"></activity>
        
    </application>

</manifest>
